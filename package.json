{
  "name": "minista",
  "description": "Next.js Like Development with 100% Static Generate",
  "version": "2.0.0-alpha.32",
  "bin": {
    "minista": "./bin/minista.js"
  },
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "lib"
  ],
  "license": "MIT",
  "homepage": "https://github.com/qrac/minista",
  "repository": {
    "type": "git",
    "url": "https://github.com/qrac/minista"
  },
  "bugs": {
    "url": "https://github.com/qrac/minista/issues"
  },
  "keywords": [
    "static-site-generator",
    "ssg",
    "coding",
    "react",
    "jsx",
    "tsx",
    "typescript",
    "esbuild",
    "vite",
    "japanese",
    "minista"
  ],
  "author": {
    "name": "Qrac",
    "url": "https://qrac.jp"
  },
  "organization": {
    "name": "QRANOKO",
    "url": "https://qranoko.jp"
  },
  "scripts": {
    "build": "tsup",
    "build:all": "tsup && tsc",
    "build:tsc": "tsc",
    "test": "vitest run",
    "test:dev": "vitest watch",
    "user": "cd ./user && npx minista",
    "user:dev": "cd ./user && npx minista",
    "user:build": "cd ./user && npx minista build",
    "user:preview": "cd ./user && npx minista preview",
    "user:v": "cd ./user && npx minista -v",
    "user:h": "cd ./user && npx minista -h",
    "prepublishOnly": "npm run clean:dist && npm run build:all",
    "clean": "npm run clean:dist && npm run clean:user",
    "clean:dist": "trash ./dist",
    "clean:user": "trash ./user/dist ./user/node_modules/.minista ./user/node_modules/.vite",
    "clean:vite": "trash ./user/node_modules/.vite"
  },
  "prettier": {
    "semi": false
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@mdx-js/esbuild": "^2.1.1",
    "@mdx-js/react": "^2.1.1",
    "@mdx-js/rollup": "^2.1.1",
    "@vitejs/plugin-react": "^1.3.0",
    "cac": "^6.7.12",
    "deepmerge-ts": "^4.0.2",
    "esbuild": "^0.14.29",
    "fs-extra": "^10.0.1",
    "js-beautify": "^1.14.2",
    "mime-types": "^2.1.35",
    "node-fetch": "^3.2.3",
    "node-html-parser": "^5.3.3",
    "picocolors": "^1.0.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.3.0",
    "rehype-highlight": "^5.0.2",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx-frontmatter": "^1.1.1",
    "srcset": "^5.0.0",
    "svgstore": "^3.0.1",
    "tiny-glob": "^0.2.9",
    "vite": "^2.9.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/js-beautify": "^1.13.3",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-helmet": "^6.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tsup": "^5.12.2",
    "typescript": "^4.6.3",
    "vitest": "^0.8.1"
  }
}
