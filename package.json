{
  "name": "minista",
  "description": "Next.js Like Development with 100% Static Generate",
  "version": "2.7.2",
  "bin": {
    "minista": "./bin/minista.js"
  },
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "lib",
    "client.d.ts"
  ],
  "license": "MIT",
  "homepage": "https://github.com/qrac/minista",
  "repository": {
    "type": "git",
    "url": "https://github.com/qrac/minista"
  },
  "bugs": {
    "url": "https://github.com/qrac/minista/issues"
  },
  "keywords": [
    "static-site-generator",
    "ssg",
    "coding",
    "react",
    "jsx",
    "tsx",
    "typescript",
    "esbuild",
    "vite",
    "japanese",
    "minista"
  ],
  "author": {
    "name": "Qrac",
    "url": "https://qrac.jp"
  },
  "organization": {
    "name": "QRANOKO",
    "url": "https://qranoko.jp"
  },
  "scripts": {
    "build": "tsup",
    "build:all": "tsup && tsc",
    "build:tsc": "tsc",
    "test": "vitest run",
    "test:dev": "vitest watch",
    "user": "cd ./user && npx minista",
    "user:dev": "cd ./user && npx minista",
    "user:dev-h": "cd ./user && npx minista --host",
    "user:build": "cd ./user && npx minista build",
    "user:preview": "cd ./user && npx minista preview",
    "user:preview-h": "cd ./user && npx minista preview --host",
    "user:v": "cd ./user && npx minista -v",
    "user:h": "cd ./user && npx minista -h",
    "prepublishOnly": "npm run clean:dist && npm run build:all",
    "clean": "npm run clean:dist && npm run clean:user",
    "clean:dist": "trash ./dist",
    "clean:user": "trash ./user/dist ./user/node_modules/.minista ./user/node_modules/.vite",
    "clean:vite": "trash ./user/node_modules/.vite"
  },
  "prettier": {
    "semi": false
  },
  "peerDependencies": {
    "less": "*",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "*",
    "stylus": "*"
  },
  "peerDependenciesMeta": {
    "less": {
      "optional": true
    },
    "sass": {
      "optional": true
    },
    "stylus": {
      "optional": true
    }
  },
  "dependencies": {
    "@mdx-js/esbuild": "^2.1.1",
    "@mdx-js/rollup": "^2.1.1",
    "@qrac/svgstore": "^3.0.3",
    "@svgr/core": "^6.2.1",
    "@vitejs/plugin-react": "^1.3.2",
    "cac": "^6.7.12",
    "deepmerge-ts": "^4.0.3",
    "esbuild": "^0.14.42",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "js-beautify": "^1.14.3",
    "mime-types": "^2.1.35",
    "mojigiri": "^0.3.0",
    "node-fetch": "^3.2.5",
    "node-html-parser": "^5.3.3",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.14",
    "postcss-modules": "^4.3.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.3.0",
    "rehype-highlight": "^5.0.2",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx-frontmatter": "^1.1.1",
    "srcset": "^5.0.0",
    "uuid": "^8.3.2",
    "vite": "^2.9.9"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/js-beautify": "^1.13.3",
    "@types/less": "^3.0.3",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^17.0.39",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.16",
    "@types/react-helmet": "^6.1.5",
    "@types/stylus": "^0.48.38",
    "@types/uuid": "^8.3.4",
    "less": "^4.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.52.2",
    "stylus": "^0.58.1",
    "tsup": "^6.0.1",
    "typescript": "^4.7.3",
    "vitest": "^0.13.1"
  }
}
