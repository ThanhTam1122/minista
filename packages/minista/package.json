{
  "name": "minista",
  "description": "Static site generator with 100% static export from React and Vite",
  "version": "3.0.0-alpha.9",
  "type": "module",
  "engines": {
    "node": ">=16.8.0"
  },
  "bin": {
    "minista": "./bin/minista.js"
  },
  "main": "dist/shared/index.js",
  "types": "dist/types/@types/index.d.ts",
  "files": [
    "bin",
    "dist",
    "client.d.ts"
  ],
  "license": "MIT",
  "homepage": "https://minista.qranoko.jp",
  "repository": {
    "type": "git",
    "url": "https://github.com/qrac/minista"
  },
  "bugs": {
    "url": "https://github.com/qrac/minista/issues"
  },
  "keywords": [
    "static-site-generator",
    "ssg",
    "coding",
    "react",
    "jsx",
    "tsx",
    "typescript",
    "esbuild",
    "vite",
    "japanese",
    "minista"
  ],
  "author": {
    "name": "Qrac",
    "url": "https://qrac.jp"
  },
  "organization": {
    "name": "QRANOKO",
    "url": "https://qranoko.jp"
  },
  "scripts": {
    "build": "npm run clean && npm run build:src && npm run build:type",
    "build:src": "tsx ../../scripts/build-sources.ts",
    "build:type": "tsc",
    "clean": "rimraf ./dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "less": "*",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0",
    "sass": "*",
    "stylus": "*"
  },
  "peerDependenciesMeta": {
    "less": {
      "optional": true
    },
    "sass": {
      "optional": true
    },
    "stylus": {
      "optional": true
    }
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.1.5",
    "@mdx-js/rollup": "^2.1.5",
    "@nuxt/devalue": "^2.0.0",
    "@qrac/svgstore": "^3.0.3",
    "@svgr/core": "^6.5.0",
    "@vitejs/plugin-react": "^2.2.0",
    "cac": "^6.7.14",
    "deepmerge-ts": "^4.2.2",
    "esbuild": "^0.15.12",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "js-beautify": "^1.14.7",
    "node-html-parser": "^6.1.1",
    "picomatch": "^2.3.1",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.4.2",
    "rehype-highlight": "^6.0.0",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx-frontmatter": "^2.0.3",
    "undici": "^5.11.0",
    "vite": "^3.2.0"
  }
}
